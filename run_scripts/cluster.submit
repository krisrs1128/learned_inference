# Test bootstrap submission
# starter submit file for CHTC jobs

universe = docker
log = job_$(Cluster).log
error = job_$(Cluster)_$(Process).err
output = job_$(Cluster)_$(Process).out
docker_image = krisrs1128/learned_inference:latest
# interactive = True

executable = train.py
arguments = -c ../conf/train.yaml -b $(Process)

should_transfer_files = YES
when_to_transfer_output = ON_EXIT

request_cpus = 1
request_memory = 1GB
request_disk = 500MB


queue 1